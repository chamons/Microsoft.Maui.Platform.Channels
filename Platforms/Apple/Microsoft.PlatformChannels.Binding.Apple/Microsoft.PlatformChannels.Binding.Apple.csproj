<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net6.0-ios;net6.0-maccatalyst</TargetFrameworks>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <EnableDefaultCompileItems>False</EnableDefaultCompileItems>
    <RootNamespace>Microsoft.PlatformChannels.Platform</RootNamespace>
    <IsBindingProject>True</IsBindingProject>
  </PropertyGroup>
  
  <ItemGroup Condition="'$(TargetFramework)' == 'net6.0-ios'">
    <NativeReference Include="..\DotNetPlatformChannels\build\DotNetPlatformChannels.xcframework\ios-arm64\DotNetPlatformChannels.framework">
        <Kind>Framework</Kind>
        <Frameworks></Frameworks>
    </NativeReference>
    <ObjcBindingApiDefinition Include="ApiDefinitions.cs" />
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'net6.0-maccatalyst' ">
    <NativeReference Include="..\DotNetPlatformChannels\build\DotNetPlatformChannels.xcframework\ios-arm64-maccatalyst\DotNetPlatformChannels.framework">
        <Kind>Framework</Kind>
        <Frameworks></Frameworks>
    </NativeReference>
    <ObjcBindingApiDefinition Include="ApiDefinitions.cs" />
  </ItemGroup>
</Project>